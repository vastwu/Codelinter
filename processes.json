{
    "apps": [{
        "name"        : "git-codereview",
        "script"      : "app.js",
        "watch"       : ["./lib/**", "app.js"],
        "max_memory_restart": "1G",
        "log_date_format" : "YYYY-MM-DD HH:mm Z",
        "error_file"  : "~/code/codelint/logs/mp.err.log",
        "out_file"  : "~/code/codelint/logs/mp.out.log",
        "env": {
          "NODE_ENV": "production",
          "PORT": "1234"
        }
    }]
}
